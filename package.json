{
  "name": "sammler-github",
  "version": "0.1.3",
  "description": "Library to fetch content from multiple GitHub repositories.",
  "scripts": {
    "eslint": "eslint lib",
    "test": "mocha --recursive --compilers js:babel-register",
    "test:unit": "mocha test/unit/**/*.spec.js --compilers js:babel-register",
    "test:e2e": "mocha test/e2e/**/*.spec.js --compilers js:babel-register",
    "build": "gulp build",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- --recursive --compilers js:babel-register",
    "coveralls": "istanbul cover _mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "homepage": "https://github.com/stefanwalther/sammler-github#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanwalther/sammler-github.git"
  },
  "author": "Stefan Walther",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stefanwalther/sammler-github/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "dependencies": {
    "bluebird": "^3.1.1",
    "del": "^2.2.0",
    "extend-shallow": "^2.0.1",
    "https": "^1.0.0",
    "js-yaml": "^3.4.6",
    "lodash": "^3.10.1",
    "lodash-deep": "^1.6.0",
    "mkdirp": "^0.5.1",
    "octonode": "^0.7.4",
    "string": "^3.3.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "chai-fs": "peterkc/chai-fs",
    "eslint": "^1.10.3",
    "fs-utils": "^0.7.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-sourcemaps": "^1.6.0",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4"
  }
}
